{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "interface-name": false
  },
  "linterOptions": {
    "exclude": [
      "node_modules/**/*.ts"
    ]
  },
  "defaultSeverity": "warning"
}
